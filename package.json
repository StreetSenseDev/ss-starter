{
  "name": "qb-starter",
  "version": "0.2.3",
  "description": "A starter kit for developing WordPress themes with Tailwind",
  "homepage": "https://github.com/mishterk/wp-tailwindcss-theme-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuallsBenson/qb-starter.git"
  },
  "keywords": [
    "wordpress",
    "wordpress-theme",
    "wordpress-starter-theme",
    "starter-kit",
    "theme",
    "tailwind",
    "laravel",
    "laravel-mix",
    "composer",
    "sass",
    "browsersync",
    "livereload"
  ],
  "scripts": {
    "dev": "mix watch",
    "hot": "mix watch --hot",
    "prod": "mix --production",
    "clean": "rm build/css/* build/js/*",
    "build": "npm run clean && npm run prod && npm run test",
    "test": "lighthouse --output-path ./tests/report.html --view $npm_package_config_url"
  },
  "author": "John Hanusek <john.hanusek@quallsbenson.com>",
  "license": "UNLICENSED",
  "main": "gulpfile.js",
  "private": true,
  "config": {
    "url": "http://wordpress.org/"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@wordpress/prettier-config": "^1.1.1",
    "browser-sync": "^2.26.10",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^6.0.3",
    "glob-all": "^3.1.0",
    "laravel-mix": "^6.0.41",
    "laravel-mix-purgecss": "^6.0.0",
    "laravel-mix-tailwind": "^0.1.2",
    "laravel-mix-versionhash": "^2.0.1",
    "lighthouse": "^9.6.8",
    "lodash": "^4.17.19",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.7",
    "sass-loader": "^10.2.1",
    "tailwindcss": "^3.0.17",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2"
  },
  "prettier": "@wordpress/prettier-config",
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "fullpage.js": "^4.0.9",
    "gsap": "^3.6.0",
    "laravel-mix-alias": "^1.0.2",
    "lottie-web": "^5.8.1",
    "smooth-scrollbar": "^8.7.4",
    "stats-js": "^1.0.1"
  }
}
